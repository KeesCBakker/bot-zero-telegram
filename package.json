{
  "name": "bot-zero",
  "version": "0.0.3",
  "private": true,
  "author": "Wehkamp (kbakker@wehkamp.nl)",
  "description": "An 'empty' Slack Bot that can be the jumpstart for your own bot. It is based on the Hubot project.",
  "dependencies": {
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "hubot": "^3.1.1",
    "hubot-command-mapper": "^2.2.2",
    "hubot-help": "^0.2.2",
    "hubot-slack": "^4.5.5"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "hubot-pretend": "^1.2.0",
    "mocha": "^5.2.0"
  },
  "engines": {
    "node": "0.10.x"
  },
  "scripts": {
    "start": "node ./index.js",
    "test": "npm run mocha ./tests/**/*.spec.js",
    "mocha": "mocha --recursive --timeout 10000 --exit"
  },
  "main": "index.js",
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wehkamp/bot-zero.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wehkamp/bot-zero/issues"
  },
  "homepage": "https://github.com/wehkamp/bot-zero#readme"
}
